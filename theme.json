          { "fontFamily": "Atlas Grotesk", "fontWeight": "400", "src": ["file:./assets/fonts/atlas-grotesk-regular.woff"] },
          { "fontFamily": "Atlas Grotesk", "fontWeight": "500", "src": ["file:./assets/fonts/atlas-grotesk-medium.woff"] },
          { "fontFamily": "Atlas Grotesk", "fontWeight": "700", "src": ["file:./assets/fonts/atlas-grotesk-bold.woff"] }
        ]
      }]
    }
  },
  "styles": {},
